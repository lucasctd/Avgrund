{
  "name": "fathom-modal",
  "version": "1.0.3",
  "description": "A modal concept which aims to give a sense of depth between the page and modal layers.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "start": "http-server ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasctd/fathom-modal.git"
  },
  "keywords": [
    "modal",
    "js",
    "deep"
  ],
  "author": "Lucas Reis <lucas@programmer.com.br>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucasctd/fathom-modal/issues"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.14",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.8"
  },
  "homepage": "https://github.com/lucasctd/fathom-modal#readme"
}
